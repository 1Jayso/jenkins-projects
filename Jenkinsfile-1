pipeline {
  agent{
    docker { image '1joe/docker-ansible:latest' }
  } 
  environment {
   ANSIBLE_PRIVATE_KEY=credentials('ssh-key')
  //  SSH_KEY=credentials('ssh-key')
  }
  stages {
    stage('Install Maria DB') {
      steps {
        // sh 'ansible-galaxy collection install -r requirements.yml'
        sh 'ansible-playbook --private-key=$ANSIBLE_PRIVATE_KEY ansible-playbooks/main.yml  --extra-vars "target=test" '
      
      }
    }
  }
}